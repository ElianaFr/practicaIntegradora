<div class="containerTit">
    <h1 class="tit">
    LISTADO DE PRODUCTOS
    </h1>
</div>
<div class="container">
{{#each products}}


    <div class="containerCard">
        {{!-- <img src="{{this.thumbnail}}"> --}}
        <div class="card">
            <img src="{{this.thumbnail}}">
            <p class="title">{{this.title}}</p>
            <p >{{this.description}}</p>
            <p>CODIGO: {{this.code}}</p>
            <p>STOCK: {{this.stock}}</pass=>
            <p class="price">PRECIO: $ {{this.price}}</p>
            <p>ID: {{this.id}}</p>
            <button class="button btnAdd">ADD</button>
            <button class="button btnInfo">+ INFO</button>
            
        </div>
    </div>
    

    
{{/each}}
</div>